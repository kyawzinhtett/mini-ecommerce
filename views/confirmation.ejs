          <div class="col-lg-12">
            <div class="details_item">
              <h4>Order Info</h4>
              <ul class="list">
                <li>
                  <a href="#"><span>Order number</span> : 60235</a>
                </li>
                <li>
                  <a href="#"><span>Date</span> : Los Angeles</a>
                </li>
                <li>
                  <a href="#"><span>Total</span> : USD 2210</a>
                </li>
                <li>
                  <a href="#"><span>Payment method</span> : Check payments</a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
